@model Professor;

<h1>Add Professor</h1>
<form asp-action="AddProfessor" method="post">
	<div class="form-group">
		<label asp-for="Id">ID</label>
		<input asp-for="Id" class="form-control"/>
	</div>
	<div class="form-group">
		<label asp-for="FirstName">First Name</label>
		<input asp-for="FirstName" class="form-control" />
	</div>
	<div class="form-group">
		<label asp-for="LastName">Last Name</label>
		<input asp-for="LastName" class="form-control" />
	</div>
	<div class="form-group">
		<label asp-for="Status">Course</label>
		<select asp-for="Status" asp-items ="Html.GetEnumSelectList(typeof(Status))" class="form-control"></select>
	</div>
	<div class="form-group">
		<label asp-for="Birthday">Date Enrolled</label>
		<input asp-for="Birthday" class="form-control" />
	</div>
	<button type ="submit" class="btn btn-primary">Submit</button>
</form>